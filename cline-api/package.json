{
  "name": "cline-api",
  "version": "1.0.0",
  "description": "REST API service based on Cline's core functionality for code generation and editing",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'No build step required'",
    "test": "node test/test-api.js",
    "docker:build": "docker build -t cline-api .",
    "docker:run": "docker run -p 3000:3000 --env-file .env cline-api"
  },
  "keywords": [
    "cline",
    "api",
    "code-generation",
    "ai",
    "llm"
  ],

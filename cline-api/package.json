  "license": "Apache-2.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "axios": "^1.8.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "diff": "^5.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.8.0",
    "node-cache": "^5.1.2",
    "openai": "^4.83.0",
    "redis": "^5.8.2",
    "uuid": "^9.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }

    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "@anthropic-ai/sdk": "^0.37.0",
    "openai": "^4.83.0",

    "axios": "^1.8.2",
    "diff": "^5.2.0",
    "uuid": "^9.0.0",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}